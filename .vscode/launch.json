    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--port",
                "9001"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "MiddleLayer: data-side-car input-init",
            "type": "python",
            "request": "launch",
            "program": "init.py",
            "env": {
                "INPUT_INIT_CONFIG": "config/input-init.json",
                "DATA_DESTINATION": "./data"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}